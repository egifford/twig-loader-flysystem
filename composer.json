{
  "name": "cedricziel/twig-loader-flysystem",
  "description": "Flysystem loader for the twig templating language",
  "keywords": ["twig", "templates", "loader", "filesystem", "flysystem"],
  "homepage": "https://github.com/cedricziel/twig-loader-flysystem",
  "require": {
    "php": "~7.4",
    "twig/twig": "^3.0",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "sllh/composer-versions-check": "^2.0",
    "phpunit/phpunit" : "~9.0",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "CedricZiel\\TwigLoaderFlysystem\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CedricZiel\\TwigLoaderFlysystem\\Test\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Cedric Ziel",
      "email": "cedric@cedric-ziel.com"
    }
  ],
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
